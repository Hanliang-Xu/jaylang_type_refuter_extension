let assert_false = 0
in let f m n =
  let rec f_loop n sum i =
    if i == 2
    then sum
    else (
        let a = n % 2
        in let sum_next = 
                sum + (if a == 0 then 0 else (a + 1))
        in let n_next = n / 2
        in f_loop n_next sum_next (i+1)
    )
  in let loop_result = f_loop n 0 0
  in let rec f_while c =
    if (loop_result == 0) and (m == 2)
    then let target = assert_false in target
    else (
      if c
      then f_while c
      else 0
    )
  in f_while true

in let m = input
in let n = input
in f m n

