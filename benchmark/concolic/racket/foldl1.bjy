# error is that input list xs should be nonempty

let rec foldl1 (f : 'a -> 'a -> 'a) (xs : List 'a) : 'a =
  match xs with
  | z :: zs ->
    match zs with 
    | [] -> z
    | z1 :: tl -> foldl1 f ((f z z1) :: tl)
    end
  | [] -> assert false # equivalent to not having this match case
  end
in
foldl1
