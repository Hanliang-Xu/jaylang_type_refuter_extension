# error is that squaring 0 or 1 doesn't make it larger like predicate requires
# Requiring n > 1 fixes the error

let rec mult (n : int) (m : int) : {. int | fun i -> i >= 0 } =
  if n <= 0 or m <= 0
  then 0
  else n + mult n (m - 1)
in

letd sqr (n : int) : {. int | fun c -> c >= n + 1 } =
  mult n n
in

sqr