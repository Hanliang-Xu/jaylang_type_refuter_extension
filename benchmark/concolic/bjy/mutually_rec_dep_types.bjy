# expect error because positive odd inputs to f first should return int by B x, but with enough recursion should return int

let B x =
  if x < 0
  then bool
  else int
in
letd rec f (x : int) : (B x) = 
    if x == 0 then 0 else g (x - 1)
with g (y : int) : (B y) =
    if y < 0 then false else f (y - 1) 
in f 1