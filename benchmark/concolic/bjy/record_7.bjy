# Expect error because a is allowed to be zero by record constraint but is positive in return type

let notPos n = n <= 0 in
let record_constraint re = 
    (notPos re.b) and
    ((re.a + re.b) >= 0)
in
let isTrue b = b in
let test (test_record : {. {: a : int, b : int :} | record_constraint }) : {. bool | isTrue } = test_record.a > 0 in
test
