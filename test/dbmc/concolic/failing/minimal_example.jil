# note that concolic gives inputs in [-10, 10] on first run.

x = input;

target_input = 100;
x_is_target_cond = x == target_input; # always false on the first run

x_is_target_branch = x_is_target_cond ? (
  first_branch_true_return = 0
) : (
  first_branch_false_return = target_input
);

second_cond = x_is_target_branch < x;

second_branch = second_cond ? (
  result_true = 42
) : (
  result_false = 84
)
