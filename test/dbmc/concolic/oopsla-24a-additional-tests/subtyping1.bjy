# "subrecords". requires wrap
# Calls a function with a supertype, so should be error. i.e. not actually subtyping here

let t = {: a : bool , b : int :}
in

let rec f (x : t) : int =
  match x with
  | { a = a , b = b } -> f { b = b } # ERROR: calls with not enough record fields 
  | { b = b } -> b 
  end
in

f
