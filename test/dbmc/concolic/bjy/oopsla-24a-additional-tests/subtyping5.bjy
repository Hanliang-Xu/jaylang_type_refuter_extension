# subtyping of functions on records

let t = ({: a : int , b : bool :} -> {: x : List int :}) -> {: y : List bool :}
in

let f (a : {: a : int  :} -> {: x : List int :}) : {: y : List bool :} =
  match (a { a = 0 }).x with
  | [] -> []
  | hd :: tl -> if hd == 0 then { y = false :: [] } else { y = true :: [] }
  end
in

let (g : t) = f # ERROR: f is supertype of t, not subtype
in

g