let B x =
  if x < 0
  then bool
  else int

let rec f (x <- int) : (B x) = 
  if x == 0
  then 0
  else g (x - 1)

and g (y <- int) : (B y) =
  if y < 0
  then false
  else f (y - 1) 

let res = f 1
