
let (obj : (((``A unit) -> int) && ((``B unit) -> int) && ((``C int) -> List int))) =
  let ob =
    fun self ->
      fun arg ->
        match arg with
        | `A u -> self self (`B {}) + 10
        | `B u -> 0
        | `C i -> self self (`A {}) :: self self (`B {}) :: [ i ]
        end
  in
  ob ob
in

obj