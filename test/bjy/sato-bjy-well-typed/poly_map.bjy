(***
  (
    (features (Polymorphic_types Recursive_functions Higher_order_functions))
    (speed Fast) ; b/c type splaying
    (typing Exhausted)
    (flags "-s")
  )
*)

let rec map (type a_ b_) (mf : a_ -> b_) (l : list a_) : list b_ =
  match l with
  | [] -> []
  | hd :: tl -> mf hd :: (map a_ b_ mf tl)
  end
