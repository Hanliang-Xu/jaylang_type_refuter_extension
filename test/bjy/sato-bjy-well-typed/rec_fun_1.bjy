(***
  (
    (features (Refinement_types Recursive_functions))
    (speed Fast)
    (typing Exhausted)
    (flags "-s")
  )
*)

let rec makelist (n : int) : list int = 
  if n == 0 then []
  else n :: (makelist (n-1))

let getHead l = 
  match l with
  | hd :: _ -> hd
  end

let main (n : int | n > 0) : { a : int | a > 0 } =
  let l = makelist n in
  if n > 0 then getHead l
  else 0
    