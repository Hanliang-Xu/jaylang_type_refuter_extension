(* ill-typed when a and b are empty *)

let is_nonempty ls =
  match ls with
  | [] -> false
  | _ -> true
  end

let rec append (type a_) (a : list a_) (b : { list a_ | is_nonempty }) : { list a_ | is_nonempty } =
  match a with
  | [] -> b
  | hd :: tl -> hd :: append a_ tl b
  end
