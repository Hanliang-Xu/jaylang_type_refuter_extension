# This is one well-typed version of `hors`

let c x = {}

let b x =
  fun y ->
    x 1

let a (x : int -> unit) (y : int -> unit) (q : int) : unit =
  let ignore = x 0 in
  y 0

let rec f n x q =
  if n <= 0
  then x q
  else a x (fun p -> f (n - 1) (fun ignore -> b x ignore) p) q

let s n q =
  f n c q

let main (n : int) : unit =
  s n n
