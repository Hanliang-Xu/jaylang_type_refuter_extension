(* error is n should be greater than 1 *)

let rec mult x y =
  if x <= 0 || y <= 0
  then 0
  else x + mult x (y - 1)

let h y =
  let b = (y + y <= mult y y)
  in
  b

let main (n : { i : int | i > 1 }) : { b : bool | b } =
  h n
