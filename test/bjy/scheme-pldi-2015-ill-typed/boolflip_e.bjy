(* error is because we can look at assert as `assert : { bool | fun b -> b } -> 'a` *)

let h x =
  assert x

let g x y =
  assert y

let rec f x y =
  if x
  then f y y (* ERROR: this is f false false => g false false => assert false *)
  else g x y

let main =
  if 0 < 1
  then f (0 < 1) (1 < 0)
  else h (1 < 0)
