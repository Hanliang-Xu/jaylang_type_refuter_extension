(*
  This test is a provably well-typed version of `mem`, but the spirit of `mem` has been ruined.
*)
let rec mk_list (dependent x : int) (n : int) : list { i : int | i == x } = 
  if n < 0
  then []
  else x :: mk_list x (n - 1)
