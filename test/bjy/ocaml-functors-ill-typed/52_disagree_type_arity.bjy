let H (dependent X : sig
  val t : type --> type
  val s : type --> type
end) (Y : sig
  val f : (a : type) -> X.s a -> a
end) : sig end = struct end

let _ = H
  (struct
    let t = abstract (* ERROR: this is wrong arity *)
    let s a = a (* this matches the expected type *)
  end)
  (struct
    let f _ x = x   (* this is fine *)
  end)
