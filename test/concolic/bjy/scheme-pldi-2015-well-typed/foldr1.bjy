# error is that xs should be nonempty

let is_nonempty ls =
  match ls with
  | [] -> false
  | _ -> true
  end
in

let rec foldr1 (f : 'a -> 'a -> 'a) (xs : {. List 'a | is_nonempty }) : 'a =
  match xs with
  | z :: zs ->
    match zs with
    | [] -> z
    | _ -> f z (foldr1 f zs)
    end 
  end
in

foldr1