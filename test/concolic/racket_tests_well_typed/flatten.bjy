
let any = Mu tt. ( int || bool || List tt ) in
let int_bool = ( int || bool ) in

let rec append a b =
  match a with
  | [] -> b
  | hd :: tl -> hd :: append tl b
  end
in

let rec flatten (x : any) : List int_bool =
  match x with
  | [] -> []
  | hd :: tl -> append (flatten hd) (flatten tl)
  | _ -> [ x ]
  end
  in

flatten
